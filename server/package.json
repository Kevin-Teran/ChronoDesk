{
  "name": "server",
  "version": "1.0.0",
  "description": "ChronoDesk API Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext js,json --exec \"npm run build && npm start\"",
    "build": "tsc",
    "lint": "eslint . --ext js,ts --fix",
    "format": "prettier --write .",
    "test": "NODE_ENV=test jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "docker:build": "docker build -t chronodesk-api .",
    "docker:run": "docker run -p 5001:5001 chronodesk-api"
  },
  "keywords": [
    "api",
    "express",
    "sequelize"
  ],
  "author": "Kevin Mariano",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.2.3",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
